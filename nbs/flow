// Crawler Flow
digraph {
	rankdir=LR
	{
		rank=same
		before [label="before_visit
callback"]
		visit [label=one_visit]
		after [label="after_visit
callback"]
	}
	start [label=Start shape=doublecircle]
	end [label=End shape=doublecircle]
	start -> before
	before -> visit
	visit -> after
	after -> end
	after -> before [constraint=false curve_style=curved]
}
